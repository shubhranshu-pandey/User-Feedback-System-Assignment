{
  "version": 2,
  "builds": [
    { "src": "backend/api/feedback.js", "use": "@vercel/node" },
    { "src": "frontend/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/feedback", "dest": "backend/api/feedback.js" },
    { "src": "/(.*)", "dest": "frontend/build/$1" }
  ]
}